{
  "name": "redux-thunk-monitor",
  "version": "1.0.0",
  "description": "redux-thunk-monitor provides an automatic, generic way to record redux-thunk loading/error disposition in your application state so that relevant consumers do not need to explicitly monitor or pass this information around to descendants.",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf lib/",
    "build": "babel -d lib/ src/ --env-name=production",
    "prepare": "npm run clean && npm run build"
  },
  "repository": "github:thomasw/redux-thunk-monitor",
  "bugs": "https://github.com/thomasw/redux-thunk-monitor/issues",
  "homepage": "https://github.com/thomasw/redux-thunk-monitor",
  "keywords": [
    "redux",
    "redux-thunk",
    "middleware",
    "redux-middleware",
    "async",
    "loading",
    "state",
    "error",
    "monitor"
  ],
  "author": "Thomas Welfley",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "flow-bin": "^0.108.0",
    "prettier": "^1.18.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended"
    ]
  },
  "prettier": {},
  "babel": {
    "presets": [
      "@babel/preset-flow",
      "@babel/preset-env"
    ]
  }
}
